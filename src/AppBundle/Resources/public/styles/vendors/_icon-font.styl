@charset "UTF-8";

@font-face {
  font-family: "klink-delivery";
  src:url("../fonts/klink-delivery.eot");
  src:url("../fonts/klink-delivery.eot?#iefix") format("embedded-opentype"),
    url("../fonts/klink-delivery.woff") format("woff"),
    url("../fonts/klink-delivery.ttf") format("truetype"),
    url("../fonts/klink-delivery.svg#klink-delivery") format("svg");
  font-weight: normal;
  font-style: normal;

}

[data-icon]:before {
  font-family: "klink-delivery" !important;
  content: attr(data-icon);
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

[class^="icon-"]:before,
[class*=" icon-"]:before {
  font-family: "klink-delivery" !important;
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

